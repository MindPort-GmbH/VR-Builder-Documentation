{
  "metadata": [
    {
      "src": [
        {
          "src": "D:/Repos/VR-Builder-UAS-2020/Assets",
          "files": [
            "**.cs"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "articles/images/**"
        ]
      }
    ],
    "output": "../docs",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "VR Builder Documentation",
      "_appTitle": "VR Builder Documentation",
      "_enableSearch": true,
      "pdf": false
    }
  }
}